<?xml version="1.0" encoding="UTF-8"?>
<suite name="CheckoutFlowValidationSuite" verbose="1">

    <listeners>
        <listener class-name="listeners.TestListener"/>
        <listener class-name="listeners.ExtentReportListener"/>
    </listeners>

    <parameter name="browser" value="chrome"/>

    <test name="CheckoutFlowTests" preserve-order="true">
        <classes>
            <class name="tests.CheckoutFlowTest">
                <methods>
                    <include name="testCheckoutFlow"/>
                </methods>
            </class>
        </classes>
    </test>

</suite>